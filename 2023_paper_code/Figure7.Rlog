HNF1A.targets <- c("HNF1A","A1CF","TMED6","S100A10","PKIB","TTR","PPP1R1A","SGK2","SPRY1","KIFC3","SPON2","SMPDL3A","BHLHE41","SMIM6","RPS9","DNAJC12","FAM107B","HOPX","NEUROD1","NKX2-2","SIX3")
